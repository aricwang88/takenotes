#cat peers/lte-cmnet 
#This is a pppd script, used by LTE-dongle
/dev/ttyUSB0
4608000
crtscts
modem
debug
nodetach
usepeerdns
defaultroute
user "cmnet"
0.0.0.0:0.0.0.0
connect '/usr/sbin/chat -s -v -f /etc/ppp/lte-chat'





